/* pages/tekken7/charactersMoveList.wxss */
.table {
  display: table;
  width: 100%;
  /* border-collapse 属性设置表格的边框是否被合并为一个单一的边框，解决相邻单元格边框未合并导致有些边框变粗的视觉效果 */
  border-collapse: collapse;
  overflow-x: hidden;
}

.table-tr {
  display: table-row;
  width: 100%;
  height: 200rpx;
}

.table-th {
  display: table-cell;
  font-weight: bold;
  border: 1px solid black;
  text-align: center;
  vertical-align: middle;
  background-color: #ccc;
}
.table-th-index {
  display: table-cell;
  font-weight: bold;
  border: 1px solid black;
  text-align: center;
  vertical-align: middle;
  background-color: #ccc;
  width: 20rpx;
}

.table-td {
  display: table-cell;
  border: 1px solid black;
  text-align: center;
  /* vertical-align: middle; */
}

.gif{
  width: 100%;
  height: 200rpx;
}

.png{
  width: 200rpx;
  height: 60rpx;
}

.image {
  width: 500rpx;
  height: 300rpx;
  /* margin: 10rpx 20rpx 0rpx 0rpx; */
  /* float: left; */
}

.window-img{
  height: auto;
  /* width: 500rpx; */
  /* height: 300rpx; */
}

.frameInfo{
  font-size: 1rem;
  color: rgb(19, 226, 226);
}